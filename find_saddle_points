def find_saddle_points(m):
    
    ans = []
    max_cols = []

    for i in range(len(m[0])):
        b = m[0][i]
        for j in range(1, len(m)):
            b = max(b, m[j][i])
        max_cols.append(b)

    for i in range(len(m)):
        for j, q in enumerate(m[i]):
            if q == min(m[i]) and q == max_cols[j]:
                ans.append((i, j))
                
    return ans
