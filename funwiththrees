class TreeNode {

    TreeNode left;
    TreeNode right;
    int value;
    
    TreeNode(int value, TreeNode left, TreeNode right) {
        this.value = value;
        this.left = left;
        this.right = right;
    }
    
    TreeNode(int value) {
        this(value, null, null);
    }
    
    @Override
    public boolean equals(Object other) {
        ... // Already implemented for you and used in test cases 
    }
}

def array_to_tree(arr):
    
    if not arr:
        return
    
    nodes = [Node(j) for j in arr]
    for i in range(1, len(arr)):
        if 2*i < len(arr):
            nodes[i-1].right = nodes[2*i]
        if 2*i-1 < len(arr):
            nodes[i-1].left = nodes[2*i-1]

    return nodes[0]
