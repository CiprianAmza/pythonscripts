import React from 'react';

export class States extends React.Component {
    
  constructor() {
    super(); 
    this.state = {
      united: false
    }
  }
  
  unite() {
    this.setState({united: true});
  }
  
  render() {
    let messages = ["Code for everyone", "Make America code again"];
    return <div className="status">{messages[this.state.united ? 0 : 1]}</div>
  } 
}
