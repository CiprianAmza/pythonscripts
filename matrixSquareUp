public class Kata {

  public static String[][] matrixSquareUp(int b) {
    
    String[][] answer = new String[b][b];
    
    for (int i = 0; i < b; i++) {
      for (int j = b-1; j >= 0; j--) {
        answer[i][j] = (b-j-1) <= i ? (b - j) + "" : "x";
      }
    }
    
    return answer;
    }
}
