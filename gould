def f(n): 
    if n <= 0:
        return 0
    elif n % 2 == 0:
        return f(n // 2)
    else:
        return f(n // 2) + 1

def solve():
    n = 0
    ans = []
    while n < 1_000_000:
        ans.append(f(n))
        n += 1
    return ans

memo = solve()
def gould():
    global memo
    return memo
