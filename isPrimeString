bool isPrimeString(String s){
  int sum = 0;
  
  for (int i = 0; i < s.length; i++) {
    sum += s.codeUnitAt(i);
  }
  
  bool isPrime(int n) {
    if (n < 2) return false;
    for (int i = 2; i * i <= n; i++) {
      if (n % i == 0) return false;
    }
    return true;
  }

  return isPrime(sum);
}
