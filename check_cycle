def check_cycle(seq, i, j):
    
    last = seq[i:j]
    while j < len(seq):
        i, j = j, j + (j-i)
        if seq[i:j] != last:
            
            return False
        last = seq[i:j]
        
    return True

def cycle(sequence):
    
    ans = []
    for i in range(len(sequence)):
        for j in range(i+1, len(sequence)):
            if check_cycle(sequence, i, j):
                return [i, j-i]
    
    return ans
