function schoolSelection(array) {
  
  let ans = {
    'Kindergarten': 0,
    '1st grade': 0,
    '2nd grade': 0, 
    '3rd grade': 0, 
    '4th grade': 0
  };
  
  for (let el of array) {
    if (el === 5) ans['Kindergarten']++;
    else if (el === 6) ans['1st grade']++;
    else if (el === 7) ans['2nd grade']++;
    else if (el === 8) ans['3rd grade']++;
    else if (el === 9) ans['4th grade']++;
  }
  
  return ans;
}
