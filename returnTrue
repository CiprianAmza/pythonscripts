public class returnTrue
{
  public static bool Way1()
  {
    return true;
  }
  public static bool Way2()
  {
    return 1 > 0;
  }
  public static bool Way3()
  {
    return !false;
  }
  public static bool Way4()
  {
    return true | false;
  }
  public static bool Way5()
  {
    return true & true;
  }
  public static bool Way6()
  {
    return true ? true : false;
  }
}
